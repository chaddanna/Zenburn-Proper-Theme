{
  "name": "Zenburn Proper",
  "type": "dark",
  "colors": {
    "editor.background": "#3F3F3F",
    "editor.foreground": "#D3D3C4",
    "editorGroup.dropBackground": "#4a4a4a",
    "activityBar.background": "#3f3f3f",
    "sideBarTitle.foreground": "#D3D3C4",
    "sideBar.background": "#3F3F3F",
    "sideBar.foreground": "#D3D3C4",
    "sideBar.dropBackground": "#4a4a4a",
    "tab.inactiveBackground": "#363636",
    "tab.border": "#363636",
    "editorGroupHeader.tabsBackground": "#363636",
    "statusBar.background": "#3F3F3F",
    "statusBar.foreground": "#D3D3C4",
    "statusBar.debuggingForeground": "#3F3F3F",
    "statusBar.debuggingBackground": "#E8C3A8",
    "statusBar.noFolderForeground": "#3F3F3F",
    "statusBar.noFolderBackground": "#94BFF3",
    "button.foreground": "#3F3F3F",
    "button.background": "#94BFF3",
    "gitDecoration.addedResourceForeground": "#82BC9D",
    "editor.selectionBackground": "#4a4a4a",
    "list.hoverBackground": "#4a4a4a",
    "list.activeSelectionBackground": "#4a4a4a",
    "list.inactiveSelectionBackground": "#4a4a4a",
    "list.dropBackground": "#4a4a4a",
    "terminal.background": "#3F3F3F",
    "terminal.foreground": "#D3D3C4",
    "terminalCursor.background": "#4a4a4a",
    "terminalCursor.foreground": "#D3D3C4",
    "terminal.ansiBlack": "#282D2A",
    "terminal.ansiBlue": "#8CB4E3",
    "terminal.ansiBrightBlack": "#647381",
    "terminal.ansiBrightBlue": "#8CB4E3",
    "terminal.ansiBrightCyan": "#83DADC",
    "terminal.ansiBrightGreen": "#A9DBA8",
    "terminal.ansiBrightMagenta": "#D9A4CB",
    "terminal.ansiBrightRed": "#BE7E80",
    "terminal.ansiBrightWhite": "#DCDCCB",
    "terminal.ansiBrightYellow": "#ECD9A0",
    "terminal.ansiCyan": "#83DADC",
    "terminal.ansiGreen": "#82BC9D",
    "terminal.ansiMagenta": "#D9A4CB",
    "terminal.ansiRed": "#BE7E80",
    "terminal.ansiWhite": "#DCDCCB",
    "terminal.ansiYellow": "#ECD9A0"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#696962"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#92E0E2"
      }
    },
    {
      "name": "Rust Operators",
      "scope": ["keyword.operator.misc.rust"],
      "settings": {
        "foreground": "#b9987f"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#D69F7D"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "keyword.control",
        "constant.other.color",
        "punctuation",
        "meta.tag",
        "punctuation.definition.tag",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "keyword.other.template",
        "keyword.other.substitution",
        "punctuation.separator.key-value"
      ],
      "settings": {
        "foreground": "#E2E1D5"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#92E0E2"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#83DADC"
      }
    },
    {
      "name": "Block Level Variables",
      "scope": ["meta.block variable.other"],
      "settings": {
        "foreground": "#92E0E2"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": ["support.other.variable", "string.other.link"],
      "settings": {
        "foreground": "#B87B7D"
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#A9DBA8"
      }
    },
    {
      "name": "Keywords",
      "scope": ["keyword.other.unit", "keyword.other"],
      "settings": {
        "foreground": "#ECD9A0"
      }
    },
    {
      "name": "Boolean",
      "scope": ["constant.language.boolean"],
      "settings": {
        "foreground": "#8CB4E3"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string",
        "punctuation.definition.string",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "#B87B7D"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name",
        "support.type",
        "support.class",
        "support.orther.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types"
      ],
      "settings": {
        "foreground": "#ECD9A0"
      }
    },
    {
      "name": "Entity Types",
      "scope": ["support.type", "entity.name.type"],
      "settings": {
        "foreground": "#94BFF3"
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#B2CCD6"
      }
    },
    {
      "name": "Sub-methods",
      "scope": [
        "entity.name.module.js",
        "variable.import.parameter.js",
        "variable.other.class.js"
      ],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Language methods",
      "scope": ["variable.language", "punctuation.accessor.js"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#E2E1D5"
      }
    },
    {
      "name": "entity.name.method.js",
      "scope": ["entity.name.method.js"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "meta.method.js",
      "scope": [
        "meta.class-method.js entity.name.function.js",
        "variable.function.constructor"
      ],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name", "meta.tag.attributes.jsx"],
      "settings": {
        "foreground": "#E2E1D5"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "text.html.basic entity.other.attribute-name.html",
        "text.html.basic entity.other.attribute-name"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FFCB6B"
      }
    },
    {
      "name": "CSS Classes",
      "scope": ["entity.other.attribute-name.class"],
      "settings": {
        "foreground": "#FFCB6B"
      }
    },
    {
      "name": "CSS ID's",
      "scope": ["source.sass keyword.control"],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#C3E88D"
      }
    },
    {
      "name": "Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#94BFF3"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js",
        "punctuation.definition.tag.begin.js",
        "punctuation.definition.tag.end.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#E2E1D5"
      }
    },
    {
      "name": "ES7 Bind Operator",
      "scope": [
        "source.js constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FF5370"
      }
    },
    {
      "name": "JSON Key - Level 0",
      "scope": [
        "source.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#94BFF3"
      }
    },
    {
